var
  dmProgs: TdmProgs;

implementation

uses uDmPrincipal, uGlobal;

{$R *.dfm}

procedure TdmProgs.cdsUsuarioBeforePost(DataSet: TDataSet);
begin
    IF cdsUsuario.State in[DsInsert] then
    begin
        cdsUsuarioIDUSUARIO.AsInteger:=Busca_Proximo('USU_USUARIOS','IDUSUARIO');

    end;
    IF cdsUsuarioADM_USUARIO.AsString='' then
    begin
        cdsUsuarioADM_USUARIO.AsString:='F';

    end;

end;

procedure TdmProgs.cdsUsuarioAfterPost(DataSet: TDataSet);
begin
   cdsUsuario.ApplyUpdates(0);

end;

procedure TdmProgs.cdsUsuarioAfterDelete(DataSet: TDataSet);
begin
   cdsUsuario.ApplyUpdates(0);

end;

procedure TdmProgs.cdsAutorizaAfterPost(DataSet: TDataSet);
begin
   cdsAutoriza.ApplyUpdates(0);
end;

procedure TdmProgs.cdsAutorizaAfterDelete(DataSet: TDataSet);
begin
   cdsAutoriza.ApplyUpdates(0);
end;

procedure TdmProgs.cdsAcessoAfterPost(DataSet: TDataSet);
begin
   cdsAcesso.ApplyUpdates(0);
end;

procedure TdmProgs.cdsEmpresaAfterPost(DataSet: TDataSet);
begin
   cdsEmpresa.ApplyUpdates(0);
end;

end.
