var
  dmVd1101: TdmVd1101;
  function Baixa_qtd_estoque(Cod_produto: Integer; Quantidade: Real; Grade: String=''; Tipo_baixa: String='-'): Boolean;

implementation

uses uDmPrincipal, uGlobal;

{$R *.dfm}

procedure TdmVd1101.cdsVendasBeforePost(DataSet: TDataSet);
begin
   if cdsVendas.State in [DsInsert] then
   begin
      cdsVendasCODIGO_VENDA.AsInteger := Busca_Proximo('P_VENDAS','CODIGO_VENDA');
      cdsVendasCOD_EMPRESA.AsInteger  := StrToInt(CodigoEmpresa1);
   end;
end;

procedure TdmVd1101.cdsVendasBeforeDelete(DataSet: TDataSet);
begin
   cdsVendasItem.Close;
   cdsVendasItem.CommandText := 'DELETE FROM P_VENDAS_ITENS WHERE CODIGO_IVENDA=' + cdsVendasCODIGO_VENDA.AsString;
   cdsVendasItem.Execute;
end;

procedure TdmVd1101.cdsDocSimplesBeforePost(DataSet: TDataSet);
begin
   if cdsDocSimples.State in [DsInsert] then
   begin
      cdsDocSimplesCODIGO_VENDA.AsInteger := Busca_Proximo('P_DOC_SIMPLES','CODIGO_VENDA');
      cdsDocSimplesCOD_EMPRESA.AsInteger  := StrToInt(CodigoEmpresa1);
   end;
end;

procedure TdmVd1101.cdsSerieAfterPost(DataSet: TDataSet);
begin
   cdsSerie.ApplyUpdates(0);
end;

procedure TdmVd1101.cdsComprasBeforePost(DataSet: TDataSet);
begin
   if cdsCompras.State in [DsInsert] then
   begin
      cdsComprasCODIGO_COMPRA.AsInteger := Busca_Proximo('P_COMPRA','CODIGO_COMPRA');
      cdsComprasCOD_EMPRESA.AsInteger   := StrToInt(CodigoEmpresa1);
   end;
end;

procedure TdmVd1101.cdsComprasBeforeDelete(DataSet: TDataSet);
begin
   cdsComprasItens.Close;
   cdsComprasItens.CommandText := 'DELETE FROM P_COMPRA_ITENS WHERE CODIGO_COMPRA=' + cdsComprasCODIGO_COMPRA.AsString;
   cdsComprasItens.Execute;
end;

function Baixa_qtd_estoque(Cod_produto: Integer;
  Quantidade: Real; Grade: String=''; Tipo_baixa: String='-'): Boolean;
Var
   xALTU2, xLARG2, xBITOLA, QTN1, ALTURA, LARGURA: Real;
begin
   Try
      QTN1 := Quantidade;

      dmvd1101.cdsProdutos.Close;
      dmvd1101.cdsProdutos.Params.ParamByName('COD_PRODUTO').AsInteger:= Cod_produto;
      dmvd1101.cdsProdutos.Open;

      if Grade <> '' then
      begin
         // Baixa na grade
         dmvd1101.cdsProdutosGrade.Close;
         dmvd1101.cdsProdutosGrade.CommandText := 'SELECT * FROM P_PRODUTOS_GRADE WHERE COD_EMPRESA=:EMPRESA AND PRODUTO_GRADE=:PRODUTO AND CODIGO_GRADE=:GRADE';
         dmvd1101.cdsProdutosGrade.Params.ParamByName('EMPRESA').AsString  := CodigoEmpresa1;
         dmvd1101.cdsProdutosGrade.Params.ParamByName('PRODUTO').AsInteger := Cod_produto;
         dmvd1101.cdsProdutosGrade.Params.ParamByName('GRADE')  .AsString  := Grade;
         dmvd1101.cdsProdutosGrade.Open;

         dmvd1101.cdsProdutosGrade.Edit;
         if Tipo_baixa = '-' then
            dmvd1101.cdsProdutosGradeESTOQUE_GRADE.AsFloat := dmvd1101.cdsProdutosGradeESTOQUE_GRADE.AsFloat - QTN1
         else
            dmvd1101.cdsProdutosGradeESTOQUE_GRADE.AsFloat := dmvd1101.cdsProdutosGradeESTOQUE_GRADE.AsFloat + QTN1;
         dmvd1101.cdsProdutosGrade.Post;

         dmvd1101.cdsProdutosGrade.ApplyUpdates(0);
      end;

      if dmvd1101.cdsVendasItemALTURA_IVENDAS.AsFloat > 0 then
      begin
         xALTU2 := dmvd1101.cdsVendasItemALTURA_IVENDAS.AsFloat  / 100;
         xLARG2 := dmvd1101.cdsVendasItemLARGURA_IVENDAS.AsFloat / 100;

         xBITOLA := (xALTU2 * xLARG2);
         QTN1 := xBITOLA * dmvd1101.cdsVendasItemQUANTIDADE_IVENDAS.AsFloat;
      end;

      // Baixa [padro]
      dmvd1101.cdsProdutos.Edit;

      if dmvd1101.cdsVendas.Active then
      begin
         if dmvd1101.cdsVendasTIPO_VENDA.AsString = 'V' then
            dmvd1101.cdsProdutosRESERVA_PRODUTO.AsFloat := dmvd1101.cdsProdutosRESERVA_PRODUTO.AsFloat - QTN1;
      end;

      if Tipo_baixa = '-' then
         dmvd1101.cdsProdutosESTOQUE_PRODUTO.AsFloat := dmvd1101.cdsProdutosESTOQUE_PRODUTO.AsFloat - QTN1
      else
         dmvd1101.cdsProdutosESTOQUE_PRODUTO.AsFloat := dmvd1101.cdsProdutosESTOQUE_PRODUTO.AsFloat + QTN1;

      dmvd1101.cdsProdutosCODIGO_PRODUTO.Value := Cod_produto;
      dmvd1101.cdsProdutosCOD_EMPRESA.AsString := CodigoEmpresa1;

      dmvd1101.cdsProdutos.Post;
      dmvd1101.cdsProdutos.ApplyUpdates(0);

   finally

   end;
end;

end.


